{
	"name": "inspiredminds/contao-wowjs",
	"description": "Integrates WOW.js for Contao content elements and form fields.",
	"keywords": ["contao", "wowjs"],
	"type": "contao-bundle",
	"homepage": "http://www.inspiredminds.at",
	"license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Fritz Michael Gschwantner",
			"homepage": "http://www.inspiredminds.at",
			"email": "fmg@inspiredminds.at",
			"role": "Developer"
		}
	],
	"support": {
		"email": "fmg@inspiredminds.at",
		"issues": "https://github.com/inspiredminds/contao-wowjs/issues",
		"source": "https://github.com/inspiredminds/contao-wowjs",
